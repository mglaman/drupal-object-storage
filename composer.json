{
    "name": "mglaman/drupal-object-storage",
    "description": "Provides integration with object storage in Drupal's stream wrappers",
    "license": "GPL-2.0-or-later",
    "type": "library",
    "authors": [
        {
            "name": "Matt Glaman",
            "email": "nmd.matt@gmail.com",
            "homepage": "https://mglaman.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3",
        "drupal/core": "^9.4 || ^10"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "ergebnis/composer-normalize": "^2.28",
        "mglaman/drupal-memory-kernel": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "mglaman\\DrupalObjectStorage\\": "src/"
        },
        "files": [
            "bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "mglaman\\DrupalObjectStorage\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        }
    }
}
